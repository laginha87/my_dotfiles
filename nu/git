
export def l [] {
  git log --oneline --decorate --graph
  }
export def s [] {
  git status
  }
export def d [] {
  git diff
  }


  export def a [] {
    git add .
  }

  export def c [msg: string] {
    git commit -m $msg
  }

 export def p [] {
  let b = current-branch
  git push origin $b
}

def current-branch [] {
  git branch --show-current | lines | first
}